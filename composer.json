{
    "name": "twoheads-digital/wink-image-manipulation",
    "description": "Adds easy implementation of PHP League's Glide image package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Luke Snowden",
            "email": "luke@twoheads.digital"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "league/glide": "^1.5",
        "league/glide-laravel": "^1.0",
        "themsaid/wink": "*"
    },
    "autoload": {
        "psr-4": {
            "WinkImageManipulation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WinkImageManipulation\\RouteServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
